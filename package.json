{
  "name": "superchat-frontend-challenge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.tsx",
    "lint:fix": "npm run lint -- --fix",
    "style:lint": "stylelint  \"./src/**/*.{css,scss,sass}\" --fix --formatter verbose",
    "build:css": "node-sass src/ -o src/",
    "watch:css": "npm run build:css && node-sass src/ -o src/ -w -r",
    "dev:profile": "cross-env NODE_ENV=development webpack serve --config config/webpack.config.js",
    "start:profile": "run-p watch:css dev:profile",
    "build:profile": "cross-env NODE_ENV=production webpack --config config/webpack.config.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest  --watch --detectOpenHandles",
    "test:coverage": "cross-env NODE_ENV=test jest  --watchAll --coverage",
    "test:update": "cross-env NODE_ENV=test jest  --updateSnapshot"
  },
  "keywords": [
    "Github",
    "typescript",
    "react",
    "webpack"
  ],
  "engines": {
    "node": ">=12.18.3"
  },
  "author": {
    "name": "Hamza Amdouni",
    "email": "amdouni.hamza.hsn@gmail.com",
    "url": "https://iamhamza.io"
  },
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged "
    }
  },
  "lint-staged": {
    "src/**/*.tsx": [
      "eslint . --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss --fix",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-colorful": "^4.4.4",
    "react-dom": "^17.0.1",
    "react-fa-icon-picker": "^1.0.3",
    "react-github-btn": "^1.2.0",
    "react-hook-form": "^6.15.8",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-proposal-numeric-separator": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/classnames": "^2.2.11",
    "@types/cssnano": "^4.0.0",
    "@types/dotenv-webpack": "^5.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
